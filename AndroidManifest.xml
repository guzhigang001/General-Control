        <activity
            android:name="com.example.grid_spinner_auto.AutoActivity"
            android:label="@string/title_activity_auto" >
        </activity>
        <activity
            android:name="com.example.grid_spinner_auto.RadioButtonActivity"
            android:label="@string/title_activity_radio_button" >
        </activity>
        <activity
            android:name="com.example.grid_spinner_auto.CheckBoxActivity"
            android:label="@string/title_activity_check_box" >
        </activity>
        <activity
            android:name="com.example.grid_spinner_auto.DialogActivity"
            android:label="@string/title_activity_dialog" >
